{
	"name": "JIS PRINT Styler",
	"version": "1.0",
	"description": "kikakurui.comで閲覧するJISの企画書を印刷する際に最適に整形します。",
	"content_scripts": [
	{
		"matches": ["https://kikakurui.com/*/*.html"],
		"exclude_matches": ["https://kikakurui.com/*/index.html"],
		"css": ["style.css"],
		"js": ["deleteAds.js"],
		"run_at": "document_start"
	}
	],
	"manifest_version": 3,
	"icons": {
		"48": "icon_48x48.png"
	}
}
